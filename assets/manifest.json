{
	"name": "Figment: a React dev tool",
	"description": "Quickly find the code for rendered React components!",
	"version": "0.5.4",
	"manifest_version": 3,
	"icons": {
		"16": "F.png",
		"32": "F.png",
		"48": "F.png",
		"128": "F.png"
	},
	"action": {
		"default_title": "Click to toggle ON/OFF"
	},
	"background": {
		"service_worker": "serviceWorker.js",
		"type": "module"
	},
	"content_scripts": [
		{
			"js": [
				"contentscript.js"
			],
			"matches": [
				"*://localhost/*"
			]
		}
	],
	"web_accessible_resources": [
		{
			"resources": [
				"Figment.js",
				"Bifrost.js",
				"elementFunctions.js",
				"Menu.js",
				"FigmentOutline.js",
				"FigmentDragable.js",
				"Trace.js",
				"styles.css",
				"dragable.css",
				"html.js"
			],
			"matches": [
				"*://localhost/*"
			]
		}
	],
	"externally_connectable": {
		"matches": [
			"http://localhost/*",
			"*://figma.com/*"
		]
	},
	"permissions": [
		"scripting",
		"sidePanel",
		"contextMenus",
		"tabs",
		"activeTab"
	],
	"host_permissions": [
		"*://localhost/*",
		"*://figma.com/*"
	],
	"commands": {
		"_execute_action": {
			"suggested_key": {
				"default": "Alt+F"
			}
		},
		"configure": {
			"suggested_key": {
				"default": "Alt+C"
			},
			"description": "Open Sidepanel"
		}
	},
	"side_panel": {
		"default_path": "sidepanel.html"
	  }
}